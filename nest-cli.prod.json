{
 "$schema": "https://json.schemastore.org/nest-cli",
 "collection": "@nestjs/schematics",
 "generateOptions": {
   "spec": false
 },
 "sourceRoot": "src",
 "compilerOptions": {
   "deleteOutDir": true,
   "watchAssets": true,
   "assets": [{
    "include": "./env/.env.production",
    "outDir": "./dist/",
    "watchAssets": true
   }]
 }
}
